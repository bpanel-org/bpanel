{
  "name": "@bpanel/bpanel",
  "version": "0.1.0-rc1",
  "homepage": "https://bpanel.org",
  "description": "GUI application to interact with a Bcoin Bitcoin node",
  "main": "server/index.js",
  "scripts": {
    "start": "node server",
    "start:dev": "node server --dev",
    "start:poll": "node server --dev --watch-poll",
    "lint": "eslint .",
    "test": "npm run lint && karma start --single-run",
    "karma": "karma start --autoWatch",
    "version": "node scripts/version.js",
    "preinstall": "node scripts/preinstall.js",
    "clear:plugins": "node server/clear-plugins.js",
    "build:plugins": "npm run clear:plugins && node server/build-plugins.js"
  },
  "lint-staged": {
    "*.{js,json,css,jsx,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:bpanel-org/bpanel.git"
  },
  "keywords": [
    "bcoin",
    "bpanel",
    "bitcoin",
    "GUI"
  ],
  "author": "bpanel",
  "license": "MIT",
  "engines": {
    "node": ">=8.9.4",
    "npm": ">=5.7.1"
  },
  "dependencies": {
    "@bpanel/bpanel-ui": "^0.0.7",
    "@bpanel/bpanel-utils": "^0.0.10",
    "aphrodite": "^1.2.5",
    "autoprefixer": "^7.1.5",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "bclient": "^0.1.0",
    "bcoin": "bcoin-org/bcoin",
    "blgr": "0.0.2",
    "bmultisig": "^0.0.2",
    "body-parser": "^1.18.2",
    "bootstrap": "^4.1.1",
    "bsock": "bcoin-org/bsock",
    "bsock-middleware": "^1.1.3",
    "bweb": "0.0.1",
    "compression-webpack-plugin": "^1.1.11",
    "cors": "^2.8.4",
    "css-loader": "^0.28.7",
    "dotenv": "^5.0.1",
    "effects-middleware": "^1.0.1",
    "eslint": "^4.9.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.1",
    "fetch-mock": "^5.13.1",
    "file-loader": "^1.1.5",
    "husky": "^0.15.0-rc.7",
    "level-js": "^2.2.4",
    "lint-staged": "^4.3.0",
    "lodash": "^4.17.10",
    "node-sass": "^4.9.2",
    "nodemon": "^1.12.1",
    "postcss-loader": "^2.0.8",
    "prettier": "^1.7.4",
    "prop-types": "^15.6.0",
    "react": "^16.3.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "sass-lint": "^1.12.1",
    "sass-loader": "^6.0.6",
    "semver": "^5.5.0",
    "source-map-loader": "^0.2.2",
    "style-loader": "^0.19.0",
    "underscore": "^1.8.3",
    "validate-npm-package-name": "^3.0.0",
    "webpack": "^3.8.1",
    "webpack-synchronizable-shell-plugin": "^0.0.7",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "karma": "^1.7.1",
    "karma-babel-preprocessor": "^7.0.0",
    "karma-chai": "^0.1.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-nyan-reporter": "^0.2.5",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.6",
    "mocha": "^4.0.1",
    "sinon": "^4.1.2"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "node scripts/version.js"
    }
  },
  "prettier": {
    "singleQuote": true
  },
  "eslintIgnore": [
    "**/*.js",
    "!server/**/*",
    "!webapp/**/*",
    "!scripts/**/*",
    "!.eslintrc.js",
    "!karma.conf.js",
    "!webpack.config.js",
    "*node_modules*"
  ]
}
