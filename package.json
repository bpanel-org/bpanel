{
  "name": "@bpanel/bpanel",
  "version": "1.0.1-alpha",
  "homepage": "https://bpanel.org",
  "description": "GUI application to interact with a Bcoin Bitcoin node",
  "main": "server/index.js",
  "scripts": {
    "start": "node --max_old_space_size=4096 server --dev --watch-poll",
    "start:dev": "node --max_old_space_size=4096 server --dev",
    "start:prod": "node --max_old_space_size=4096 server",
    "start:poll": "npm run start",
    "version": "node scripts/version.js",
    "preinstall": "node scripts/preinstall.js",
    "clean": "npm run clear:plugins && rm -rf ./dist/*",
    "clear:plugins": "node server/clear-plugins.js",
    "build:plugins": "npm run clear:plugins && node server/build-plugins.js",
    "build:dll": "webpack --config ./configs/webpack.dll.config.js",
    "lint": "eslint webapp/ server/ configs/ scripts/",
    "test": "karma start configs/karma.conf.js",
    "test:watch": "karma start configs/karma.conf.js --autoWatch",
    "test:server": "mocha --reporter spec --watch server/test"
  },
  "lint-staged": {
    "*.{js,json,css,jsx,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:bpanel-org/bpanel.git"
  },
  "keywords": [
    "bcoin",
    "bpanel",
    "bitcoin",
    "GUI"
  ],
  "author": "bpanel",
  "license": "MIT",
  "engines": {
    "node": ">=8.9.4",
    "npm": ">=5.7.1"
  },
  "dependencies": {
    "@bpanel/bpanel-ui": "0.0.19",
    "@bpanel/bpanel-utils": "0.1.9",
    "aphrodite": "1.2.5",
    "autoprefixer": "9.4.5",
    "base64-inline-loader": "1.1.1",
    "bcash": "bcoin-org/bcash",
    "bcfg": "0.1.4",
    "bclient": "0.1.7",
    "bcoin": "bcoin-org/bcoin",
    "bcrypto": "~4.3.2",
    "bcurl": "0.1.4",
    "bfile": "~0.1.3",
    "binet": "~0.3.3",
    "bledger": "~0.1.5",
    "blgr": "0.1.4",
    "bmultisig": "2.0.0-beta.1",
    "body-parser": "1.18.2",
    "bootstrap": "4.3.1",
    "bsert": "~0.0.5",
    "bsock": "bucko13/bsock#paths",
    "bsock-middleware": "1.1.5",
    "bstring": "~0.3.4",
    "bval": "^0.1.5",
    "bweb": "~0.1.6",
    "compression": "1.7.3",
    "cors": "2.8.4",
    "css-loader": "2.1.0",
    "effects-middleware": "1.0.1",
    "express": "4.16.2",
    "file-loader": "2.0.0",
    "font-awesome": "4.7.0",
    "hs-client": "~0.0.4",
    "hsd": "handshake-org/hsd#b75e48e5158b2157fb9f7d040dd4951f7d0f47fe",
    "husky": "0.15.0-rc.7",
    "level-js": "2.2.4",
    "lodash": "^4.17.15",
    "nodemon": "1.18.10",
    "postcss-loader": "3.0.0",
    "prettier": "1.7.4",
    "prop-types": "15.6.0",
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "react-redux": "5.0.6",
    "react-router-dom": "4.3.1",
    "redux": "3.7.2",
    "redux-devtools-extension": "2.13.2",
    "redux-persist": "5.10.0",
    "redux-thunk": "2.2.0",
    "reselect": "3.0.1",
    "semver": "5.5.0",
    "style-loader": "0.23.1",
    "validate-npm-package-name": "3.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.0.1",
    "babel-loader": "7.1.2",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-runtime": "6.26.0",
    "chai": "4.1.2",
    "compression-webpack-plugin": "3.1.0",
    "eslint": "4.18.2",
    "eslint-config-prettier": "2.6.0",
    "eslint-import-resolver-webpack": "0.8.3",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-prettier": "2.3.1",
    "eslint-plugin-react": "7.4.0",
    "html-webpack-plugin": "3.2.0",
    "karma": "3.0.0",
    "karma-babel-preprocessor": "7.0.0",
    "karma-chai": "0.1.0",
    "karma-firefox-launcher": "1.1.0",
    "karma-mocha": "1.3.0",
    "karma-nyan-reporter": "0.2.5",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-sinon": "1.0.5",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "4.0.0-rc.5",
    "lint-staged": "4.3.0",
    "mini-css-extract-plugin": "0.4.2",
    "mocha": "4.0.1",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "sinon": "4.1.2",
    "source-map-loader": "0.2.2",
    "uglifyjs-webpack-plugin": "2.1.1",
    "webpack": "^4.41.5",
    "webpack-cli": "3.1.0",
    "webpack-merge": "4.1.4",
    "webpack-synchronizable-shell-plugin": "0.0.7"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "node scripts/version.js"
    }
  },
  "prettier": {
    "singleQuote": true
  },
  "eslintIgnore": [
    "**/*.js",
    "!server/**/*",
    "!webapp/**/*",
    "!scripts/**/*",
    "!.eslintrc.js",
    "!karma.conf.js",
    "!configs/*.config.js",
    "*node_modules*"
  ]
}
